<table>
  <tr>
    <% %w{provider uid name admin}.each do |field| %>
      <th><%=t ".#{field}" %></th>
    <% end %>
    <th colspan="3"></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%=t "sessions.new.#{user.provider}" %></td>
    <td><%= user.uid %></td>
    <td><%= user.name %></td>
    <td><%= user.admin %></td>
    <td><%= link_to t(:edit), edit_user_path(user) %></td>
    <td><%= link_to t(:destroy), user, :confirm => t(:confirm), :method => :delete %></td>
  </tr>
<% end %>
</table>
